function room_AfterFadeIn()
{
	Overlay *textOverlay;
	
	textOverlay = Overlay.CreateTextual(100, 115,  200, eFontNormal, 7, "Obrigado por jogar!");
	Wait(100);
	
	textOverlay = Overlay.CreateTextual(100, 115,  200, eFontNormal, 7, "");
	Wait(300);
	QuitGame(0);


}
